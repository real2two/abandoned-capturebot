{
  "name": "pokemon-but-with-real-people",
  "type": "module",
  "author": "Two",
  "main": "src/index.js",
  "scripts": {
    "start": "node -r dotenv/config .",
    "dev": "nodemon",
    "format": "biome format --write ./",
    "commands/create": "node -r dotenv/config tools/createCommands.js"
  },
  "nodemonConfig": {
    "ignore": [".git", "node_modules"],
    "watch": ["./src"],
    "exec": "npm start",
    "ext": "ts, js"
  },
  "dependencies": {
    "@biomejs/biome": "^1.7.0",
    "@discordjs/builders": "^1.7.0",
    "discord-interactions": "^3.4.0",
    "dotenv": "^16.4.5",
    "hyper-express": "^6.15.2",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.0"
  },
  "devDependencies": {
    "discord-api-types": "^0.37.79"
  }
}
